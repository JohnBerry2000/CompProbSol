    RowBox[{
     RowBox[{
      RowBox[{"V", "[", "x_", "]"}], ":=", 
      SuperscriptBox["x", "2"]}], ";", 
     RowBox[{"sol", " ", ":=", " ", 
      RowBox[{"DSolve", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           FractionBox[
            RowBox[{"-", "\[HBar]"}], 
            RowBox[{"2", " ", "m"}]], 
           RowBox[{
            RowBox[{"\[Psi]", "''"}], "[", "x", "]"}]}], "+", 
          RowBox[{
           RowBox[{"V", "[", "x", "]"}], 
           RowBox[{"\[Psi]", "[", "x", "]"}]}]}], "\[Equal]", 
         RowBox[{"E", " ", 
          RowBox[{"\[Psi]", "[", "x", "]"}]}]}], ",", 
        RowBox[{"\[Psi]", "[", "x", "]"}], ",", "x"}], "]"}]}], ";", " ", 
     RowBox[{"Return", "[", 
      RowBox[{
       RowBox[{"\[Psi]", "[", "x", "]"}], "/.", 
       RowBox[{"sol", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "1"}], "]"}], "]"}]}], "]"}]}]}], "]"}]}]], "Input",
