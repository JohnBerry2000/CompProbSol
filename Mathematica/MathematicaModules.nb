  RowBox[{"wave", "[", "m_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[HBar]", "=", 
      RowBox[{"6.60", "*", 
       SuperscriptBox["10", 
